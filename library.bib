pages = {1495--1512},
pmid = {28620816},
publisher = {Springer Science and Business Media B.V.},
title = {{Bioaccessibility of As, Cu, Pb, and Zn in mine waste, urban soil, and road dust in the historical mining village of Kaňk, Czech Republic}},
