issn = {1572-896X},
journal = {Journal of Nanoparticle Research 2005 7:4},
keywords = {Characterization and Evaluation of Materials,Inorganic Chemistry,Lasers,Nanotechnology,Optical Devices,Optics,Photonics,Physical Chemistry,Water quality},
month = {oct},
