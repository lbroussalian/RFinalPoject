issn = {0006-3568},
journal = {BioScience},
keywords = {biodiversity,biogeochemistry,flow cessation,fragmentation,habitat mosaics},
month = {mar},
number = {3},
pages = {229--235},
publisher = {Oxford Academic},
title = {{Intermittent Rivers: A Challenge for Freshwater Ecology}},
url = {https://academic.oup.com/bioscience/article/64/3/229/224292},
volume = {64},
year = {2014}
}
@article{Mohajerin2014,
abstract = {Tungsten (W) concentrations were measured along with arsenic (As) in groundwaters from the Murshidabad district of West Bengal, India. Tungsten concentrations range from 0.8 to {\~{}}8 nmol kg-1 (0.15- 1.5 ?g kg-1) in the circumneutral pH (average pH{\~{}}7.3) Murshidabad groundwaters, and attain concentrations as high as 14 nmol kg-1 (2.5 ?g kg-1) in local ponds (n=2). Total dissolved As concentrations (AsT) range from 0.013 to 53.9 ?mol kg-1 ({\textless}1 to 4,032 ?g kg-1), and As(III) predominates in Murshidabad groundwaters accounting for 70 {\%}, on average, of As in solution. Tungsten concentrations in Murshidabad groundwaters are low compared to alkaline groundwaters (pH{\textgreater}8) from the Carson Desert inWesternNevada, USA, where W concentrations are reported to reach as high as 4,036 nmol kg-1 (742 ?g kg-1). AlthoughWis positively correlated with As in groundwaters from the Carson Desert, it is not correlated with AsT or As(III) in Murshidabad groundwaters, but does exhibit a weak relationship with As(V) in these groundwaters. Surface complexation modeling indicates that pH related adsorption/desorption can explain the geochemical behavior of W in Murshidabad groundwaters. However, the model does not predict the high As concentrations observed in Murshidabad groundwaters. The high As and low W concentrations measured in Murshidabad groundwaters indicate that either As and W originate from different sources or are mobilized by different biogeochemical processes within the Murshidabad aquifers. Mobilization of As in Murshidabad groundwaters is presumed to reflect reductive dissolution of Fe(III) oxides/oxyhydroxides and release of sorbed and/or coprecipitated As to the groundwaters. Multivariate statistical analysis of groundwater composition data indicate thatWis associated with Mn and Cl-, which may point to a Mn oxide/oxyhydroxide, clay mineral, and/or apatite source for Win the Murshidabad sediments. {\textcopyright} Springer Science+Business Media Dordrecht 2013.},
author = {Mohajerin, T. Jade and Neal, Andrew W. and Telfeyan, Katherine and Sasihharan, Sankar M. and Ford, Sophie and Yang, Ningfang and Chevis, Darren A. and Grimm, Deborah A. and Datta, Saugata and White, Christopher D. and Johannesson, Karen H.},
doi = {10.1007/S11270-013-1792-X},
file = {:C$\backslash$:/Users/Lilianna Broussalian/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Mohajerin et al. - 2014 - Geochemistry of tungsten and arsenic in aquifer systems A comparative study of groundwaters from west bengal,.pdf:pdf},
issn = {00496979},
journal = {Water, Air, and Soil Pollution},
