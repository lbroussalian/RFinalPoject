file = {:C$\backslash$:/Users/Lilianna Broussalian/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Violante et al. - 2010 - Mobility and bioavailability of heavy metals and metalloids in soil environments(2).pdf:pdf},
issn = {07189516},
journal = {Journal of Soil Science and Plant Nutrition},
keywords = {Adsorption,Bioavailability,Chemical speciation,Desorption,Heavy metals,Metalloid},
number = {3},
pages = {268--292},
publisher = {Sociedad Chilena de la Ciencia del Suelo},
title = {{Mobility and bioavailability of heavy metals and metalloids in soil environments}},
volume = {10},
year = {2010}
