file = {:C$\backslash$:/Users/Lilianna Broussalian/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Hseu et al. - 2002 - Digestion methods for total heavy metals in sediments and soils.pdf:pdf},
issn = {00496979},
journal = {Water Air Soil Pollut},
keywords = {Digestion method,Heavy metal,Sediment,Soil,Total analysis},
month = {nov},
