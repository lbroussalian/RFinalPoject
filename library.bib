title = {{Determination of mass-dependent variations in tungsten stable isotope compositions of geological reference materials by double-spike and MC-ICPMS}},
url = {https://pubs.rsc.org/en/content/articlehtml/2015/ja/c5ja00210a https://pubs.rsc.org/en/content/articlelanding/2015/ja/c5ja00210a},
